<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.will.nafila">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BoundaryClasses.ActivitiesClasses.MainActivity" />
        <activity android:name=".BoundaryClasses.ActivitiesClasses.BeginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BoundaryClasses.ActivitiesClasses.LoginActivity" />
        <activity android:name=".BoundaryClasses.ActivitiesClasses.RegisterActivity" />
        <activity android:name=".BoundaryClasses.ActivitiesClasses.HomeActivity" />
        <activity android:name=".BoundaryClasses.ActivitiesClasses.UserViewActivity" />
        <activity android:name=".BoundaryClasses.ActivitiesClasses.MenuActivity" />
        <activity android:name=".BoundaryClasses.ActivitiesClasses.NewsActivity" />
        <activity android:name=".BoundaryClasses.ActivitiesClasses.ShowUserActivity" />
        <activity android:name=".BoundaryClasses.ActivitiesClasses.EditProfileActivity" />
        <activity android:name=".BoundaryClasses.ActivitiesClasses.ContactActivity"></activity>
    </application>

</manifest>